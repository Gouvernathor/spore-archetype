{
  "name": "spore-archetype",
  "version": "1.0.1",
  "description": "Represents the various rules for space-stage Archetypes based on previous alignment choices in the 2008 game Spore.",
  "keywords": [
    "spore",
    "alignment"
  ],
  "homepage": "https://github.com/Gouvernathor/spore-archetype#readme",
  "bugs": {
    "url": "https://github.com/Gouvernathor/spore-archetype/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gouvernathor/spore-archetype.git"
  },
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-or-later",
  "author": "Gouvernathor",
  "type": "module",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "tsc",
    "build:css": "sass src/colors-classed.scss:dist/colors-classed.css src/colors-root.scss:dist/colors-root.css",
    "test": "vitest",
    "10force": "npm run build:js && node boot/10force.js"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "sass": "^1.97.2",
    "typescript": "*",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "jsdom": "^27.4.0"
},
  "peerDependenciesMeta": {
    "jsdom": {
      "optional": true
    }
  }
}
